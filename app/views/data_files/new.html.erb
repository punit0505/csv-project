<%= form_for @data_file, url: data_files_path, html: { multipart: true } do |form| %>
  <div class="form-group row">
    <%= form.label :csv_upload, class: "col-sm-2 col-form-label" %>
    <div class="col-sm-10">
      <%= form.file_field :csv_upload, class: 'form-control-file' %>
    </div> 
  </div> 
  <div class="form-group row">
    <%= form.label :emails, class: "col-sm-2 col-form-label" %>
    <div class="col-sm-10">
      <%= form.text_field :emails, id: "tokenfield", class: 'form-control' %>
    </div> 
  </div>
 
  <div class="form-group row">
  	<div class="col-sm-10">
       <%= form.submit 'Submit', class: "btn btn-primary mb-2" %>
    </div>   
  </div>
<% end %>
